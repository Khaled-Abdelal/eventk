{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 0,
        "no-unused-vars": [
            2
        ],
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 8
            }
        ]
    }
}